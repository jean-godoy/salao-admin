CREATE TABLE `brothers_style`.`clientes` ( `id_cliente` INT NOT NULL AUTO_INCREMENT , `nome` VARCHAR(100) NOT NULL , `sexo` INT NOT NULL , `data_nasc` DATE NOT NULL , `endereco` VARCHAR(100) NOT NULL , `numero` INT NOT NULL , `bairro` VARCHAR(100) NOT NULL , `cidade` VARCHAR(100) NOT NULL , `estado` VARCHAR(50) NOT NULL , `telefone` FLOAT NOT NULL , `rg` INT NOT NULL , `cpf` INT(11) NOT NULL , `email` VARCHAR(100) NOT NULL , `foto` VARCHAR(100) NOT NULL , `data_reg` INT NOT NULL , PRIMARY KEY (`id_cliente`)) ENGINE = MyISAM;
